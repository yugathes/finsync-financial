# E2E Testing Environment Variables Example
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Required for E2E tests)
VITE_REACT_APP_SUPABASE_URL=https://your-project.supabase.co
VITE_REACT_APP_SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Database Configuration (Required)
DATABASE_URL=postgresql://username:password@localhost:5432/finsync_db

# Playwright Test Configuration (Optional - defaults provided)
PLAYWRIGHT_BASE_URL=http://localhost:5000

# Node Environment
NODE_ENV=development
